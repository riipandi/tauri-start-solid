{
    "name": "tauri-tray-app",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "tauri dev",
        "build": "tauri build",
        "build:mac:x64": "CI=true tauri build --target x86_64-apple-darwin",
        "build:mac:arm": "CI=true tauri build --target aarch64-apple-darwin",
        "build:mac:universal": "CI=true tauri build --target universal-apple-darwin",
        "build:win:x64": "CI=true tauri build --target x86_64-pc-windows-msvc",
        "build:win:arm": "CI=true tauri build --target aarch64-pc-windows-msvc",
        "build:debug": "tauri build --debug",
        "build:ui": "vite build",
        "dev:ui": "vite",
        "generate:icons": "tauri icon ./src/assets/images/app-icon.png",
        "generate:signkey": "tauri signer generate -w .tauri/${npm_package_name}_signer.key",
        "depgraph": "(cd src-tauri && cargo depgraph --build-deps --frozen | dot -Tpng > ../depgraph.png)",
        "cleanup": "rimraf dist pnpm-lock.yaml src-tauri/target src-tauri/Cargo.lock node_modules",
        "update-deps": "npm-check-updates --configFileName .ncurc.json",
        "format:rs": "(cd src-tauri && cargo fmt)",
        "format:js": "biome format . --write",
        "format": "pnpm run \"/format:/\"",
        "check": "biome check --apply .",
        "lint": "biome lint --apply .",
        "release:major": "rtk release -r major",
        "release:minor": "rtk release -r minor",
        "release:patch": "rtk release -r patch",
        "test:js": "echo 'Not yet implemented'",
        "test:rs": "(cd src-tauri && cargo nextest run)",
        "tauri": "tauri"
    },
    "dependencies": {
        "@tauri-apps/api": ">=2.0.0-beta.6",
        "@tauri-apps/plugin-dialog": ">=2.0.0-beta.3",
        "@tauri-apps/plugin-fs": ">=2.0.0-beta.3",
        "@tauri-apps/plugin-http": ">=2.0.0-beta.3",
        "@tauri-apps/plugin-log": ">=2.0.0-beta.4",
        "@tauri-apps/plugin-notification": ">=2.0.0-beta.3",
        "@tauri-apps/plugin-os": ">=2.0.0-beta.3",
        "@tauri-apps/plugin-process": ">=2.0.0-beta.3",
        "@tauri-apps/plugin-shell": ">=2.0.0-beta.3",
        "@tauri-apps/plugin-updater": ">=2.0.0-beta.3",
        "@tauri-apps/plugin-window-state": ">=2.0.0-beta.4",
        "clsx": "^2.1.1",
        "lucide-react": "^0.377.0",
        "react-dom": "^18.3.1",
        "react-hotkeys-hook": "^4.5.0",
        "react-router-dom": "^6.23.0",
        "react": "^18.3.1",
        "tailwind-merge": "^2.3.0",
        "zustand": "^4.5.2"
    },
    "devDependencies": {
        "@biomejs/biome": "1.7.2",
        "@tailwindcss/vite": "4.0.0-alpha.10",
        "@tauri-apps/cli": ">=2.0.0-beta.11",
        "@types/node": "^20.12.8",
        "@types/react-dom": "^18.3.0",
        "@types/react": "^18.3.1",
        "@vitejs/plugin-react": "^4.2.1",
        "keep-a-changelog": "^2.5.3",
        "lefthook": "^1.6.10",
        "npm-check-updates": "^16.14.20",
        "rimraf": "^5.0.5",
        "rtk": "^3.1.1",
        "tailwindcss": "4.0.0-alpha.10",
        "typescript": "^5.4.5",
        "vite": "^5.2.11"
    }
}
