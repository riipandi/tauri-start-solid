{
    "name": "tauri-tray-app",
    "version": "2.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "tauri dev",
        "build": "tauri build",
        "dev:ui": "NODE_ENV=development vite",
        "build:mac:x64": "CI=true tauri build --target x86_64-apple-darwin",
        "build:mac:arm": "CI=true tauri build --target aarch64-apple-darwin",
        "build:mac:universal": "CI=true tauri build --target universal-apple-darwin",
        "build:win:x64": "CI=true tauri build --target x86_64-pc-windows-msvc",
        "build:win:arm": "CI=true tauri build --target aarch64-pc-windows-msvc",
        "build:debug": "tauri build --debug",
        "build:ui": "vite build",
        "check": "biome check . --write",
        "format:rs": "(cd src-tauri && cargo fmt)",
        "format:js": "biome format . --write",
        "format": "pnpm --stream --color \"/format:/\"",
        "lint": "biome lint . --write",
        "generate:icons": "tauri icon ./src/assets/images/app-icon.png",
        "generate:signkey": "tauri signer generate -w .tauri/${npm_package_name}_signer.key",
        "depgraph": "(cd src-tauri && cargo depgraph --build-deps --frozen | dot -Tpng > ../depgraph.png)",
        "update-deps": "npm-check-updates --configFileName .ncurc.json",
        "cleanup": "pnpm --stream --parallel --color \"/cleanup:(?!deps)/\"",
        "cleanup:deps": "pnpm dlx rimraf pnpm-lock.yaml node_modules src-tauri/Cargo.lock",
        "cleanup:dev": "pnpm dlx rimraf .output dist src-tauri/target",
        "release:major": "rtk release -r major",
        "release:minor": "rtk release -r minor",
        "release:patch": "rtk release -r patch",
        "test:js": "echo 'Not yet implemented'",
        "test:rs": "(cd src-tauri && cargo nextest run)",
        "typecheck": "tsc -b --noEmit",
        "tauri": "tauri"
    },
    "dependencies": {
        "@ark-ui/solid": "^4.5.1",
        "@corvu/drawer": "^0.2.2",
        "@corvu/otp-field": "^0.1.4",
        "@corvu/resizable": "^0.2.3",
        "@kobalte/core": "^0.13.7",
        "@nanostores/persistent": "^0.10.2",
        "@nanostores/solid": "^0.5.0",
        "@solidjs/meta": "^0.29.4",
        "@solidjs/router": "^0.15.1",
        "@tanstack/solid-table": "^8.20.5",
        "@tauri-apps/api": "^2",
        "@tauri-apps/plugin-http": "^2.0.1",
        "@tauri-apps/plugin-shell": "^2",
        "cmdk-solid": "^1.1.0",
        "embla-carousel-solid": "^8.5.1",
        "lucide-solid": "^0.468.0",
        "nanostores": "^0.11.3",
        "pathe": "^1.1.2",
        "solid-js": "^1.9.3",
        "solid-sonner": "^0.2.8",
        "std-env": "^3.8.0",
        "tailwindcss-animate": "^1.0.7"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@tauri-apps/cli": "^2",
        "@types/node": "^22.10.1",
        "autoprefixer": "^10.4.20",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "keep-a-changelog": "^2.5.3",
        "lefthook": "^1.8.5",
        "npm-check-updates": "^17.1.11",
        "postcss": "^8.4.49",
        "rtk": "^3.1.1",
        "tailwind-merge": "^2.5.5",
        "tailwindcss": "^3.4.16",
        "typescript": "~5.7.2",
        "vite": "^6.0.3",
        "vite-plugin-solid": "^2.11.0",
        "vite-tsconfig-paths": "^5.1.3"
    },
    "packageManager": "pnpm@9.14.4"
}
